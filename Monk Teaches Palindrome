T = int(input())
for t in range(0,T):
    s = input()
    n = len(s)
    f = 0
    for i in range(0,n//2):
        if s[i]!=s[n-i-1]:
            f = 1
            break
    if(f==1):
        print('NO')
    else:
        if n%2==0:
            print('YES EVEN')
        else:
            print('YES ODD')
    
